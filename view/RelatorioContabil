class RelatorioContabil {
    private TaxaContabil taxaContabil;

    public RelatorioContabil(TaxaContabil taxaContabil) {
        this.taxaContabil = taxaContabil;
    }

    public void gerarRelatorio() {
        System.out.println("Relatório de Taxa Contábil: " + taxaContabil.calcularTaxa());
    }
}